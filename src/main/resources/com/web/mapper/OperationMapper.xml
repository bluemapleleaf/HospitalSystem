  <select id="queryPatientUserRoom" parameterType="com.web.entity.Operation" 
     resultMap="resultOperation">
     SELECT o.*,p.*,u.*,r.*
     FROM operation o,patient p,inpatient i,user u,operationroom r
     WHERE o.hospitalizationId = i.hospitalizationId and p.patientId = i.patientId
     AND o.chiefSurgeonId = u.userId
     AND o.operationRoomId = r.operationRoomId
